        <CustomUIElement type="HudTopBar"	layoutfile="file://{resources}/layout/custom_game/frostivus_time.xml" />
	<CustomUIElement type="Hud"		layoutfile="file://{resources}/layout/custom_game/frostivus_notifications.xml" />
	</Panel>
</root>